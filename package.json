{
  "name": "video-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/cypress": "^5.0.1",
    "antd": "^3.19.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "cypress": "3.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "googleapis": "^40.0.0",
    "js-cookie": "^2.2.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "prettier": "1.17.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "serve": "^11.1.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development && concurrently --kill-others-on-fail \"yarn server\" \"yarn start\"",
    "serve": "cross-env NODE_ENV=production && concurrently --kill-others-on-fail \"nodemon ./server/server.js\" \"serve -s build\"",
    "server": "nodemon ./server/server.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier \"**/*.js\" --write",
    "test:e2e": "node ./e2e/run.js",
    "cypress:open": "concurrently \"npm run serve\" \"cypress open --project ./e2e\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
